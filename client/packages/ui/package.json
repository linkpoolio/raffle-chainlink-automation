{
  "name": "raffle-chainlink-automation-ui",
  "version": "0.1.0",
  "private": true,
  "browserslist": [
    "defaults"
  ],
  "scripts": {
    "start": "vite",
    "start:docker": "vite --host 0.0.0.0",
    "build": "vite build --emptyOutDir",
    "serve": "vite preview --port 3004",
    "prettier": "prettier --check \"src/**/**/*.{js,jsx,ts,tsx,html}\"",
    "prettier:fix": "prettier --write \"src/**/**/*.{js,jsx,ts,tsx,html}\"",
    "lint": "eslint src --ext .js,.jsx,.ts,.tsx",
    "lint:fix": "eslint --fix src --ext .js,.jsx,.ts,.tsx",
    "test": "jest --transformIgnorePatterns \"node_modules/(?!@toolz/allow)/\" --env=jsdom",
    "tsc": "tsc --noEmit",
    "migrate": " ts-jest config:migrate ./jest.config.js"
  },
  "dependencies": {
    "@react-hookz/web": "^12.1.2",
    "ethers": "5.5.4",
    "react": "latest",
    "react-dom": "latest",
    "react-helmet": "^6.1.0",
    "react-router": "5.3.1",
    "react-router-dom": "5.3.1",
    "wagmi": "^0.11.5"
  },
  "devDependencies": {
    "@babel/core": "^7.15.0",
    "@babel/plugin-transform-runtime": "^7.15.0",
    "@babel/preset-env": "^7.15.4",
    "@babel/preset-react": "^7.14.5",
    "@babel/preset-typescript": "^7.10.4",
    "@testing-library/jest-dom": "^5.14.1",
    "@testing-library/react": "^12.0.0",
    "@testing-library/user-event": "^13.2.1",
    "@types/jest": "^29.1.2",
    "@types/node": "^18.13.0",
    "@types/react": "^18.0.21",
    "@types/react-dom": "latest",
    "@typescript-eslint/eslint-plugin": "^5.51.0",
    "@typescript-eslint/parser": "^5.51.0",
    "@vitejs/plugin-react-refresh": "^1.3.1",
    "babel-jest": "^29.1.2",
    "babel-loader": "^8.2.2",
    "babel-plugin-module-resolver": "^5.0.0",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^3.4.0",
    "eslint-plugin-react": "^7.24.0",
    "eslint-plugin-testing-library": "^4.12.0",
    "jest": "^29.1.2",
    "jest-environment-jsdom": "^29.1.2",
    "jest-transform-stub": "^2.0.0",
    "prettier": "^2.4.0",
    "react-test-renderer": "^18.2.0",
    "ts-jest": "^29.0.5",
    "tsconfig-paths-jest": "^0.0.1",
    "typescript": "4.9.5",
    "vite": "^2.5.4",
    "vite-plugin-react-md": "^1.0.1"
  }
}
